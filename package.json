    "license": "MIT",
    "author": "NAV",
    "scripts": {
        "build": "node src/build/scripts/production-build.js",
        "deploy": "gh-pages -d build",
        "start-express": "node ./server.js",
        "start": "node ./src/build/scripts/start-dev.js",
        "dev": "npm-run-all -p rm:dist dev-api snd",
        "dev-api": "PORT=8099 node ./mocks/server.js",
        "prepare": "npm run snyk-protect",
        "rm:dist": "rimraf dist/",
        "snyk-protect": "snyk-protect",
        "snd": "node ./src/build/scripts/start-dev-no-decorator.js",
        "test": "jest --maxWorkers=2 --config ./jest/jest.config.js",
        "test-w": "jest --maxWorkers=2 --config ./jest/jest.config.js --watch",
        "test-c": "jest  --coverage --config ./jest/jest.config.js --watch",
        "msw-init": "msw init dist",
        "cypress:headless": "cypress run",
        "e2e:start-server": "MSW_MODE=test node --icu-data-dir=node_modules/full-icu src/build/scripts/start-dev-no-decorator.js",
        "e2e:setup": "start-server-and-test e2e:start-server http://localhost:8090 cypress:headless",
        "e2e:run": "npm-run-all -p msw-init e2e:setup",
        "e2e:run2": "npm-run-all -p msw-init snd"
    },
